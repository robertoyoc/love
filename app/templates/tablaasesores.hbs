

        <!-- begin MAIN PAGE CONTENT -->
        <div id="page-wrapper">

            <div class="page-content">

                <!-- begin PAGE TITLE ROW -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="page-title">
                            <h1>Lista de Asesores
                            </h1>
                            <ol class="breadcrumb">
                                <li><i class="fa fa-dashboard"></i>  <a href="principal">Inicio</a>
                                </li>
                                <li class="active">Tabla de asesores</li>
                            </ol>
                        </div>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <!-- end PAGE TITLE ROW -->

                <!-- begin ADVANCED TABLES ROW -->
                <div class="row">

                    <div class="col-lg-12">

                        <div class="portlet portlet-default">
                            <div class="portlet-heading">
                                <div class="portlet-title">
                                    <h4>Asesores registrados</h4>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="portlet-body">
                                <div class="table-responsive">
                                    <table id="example-table" class="table table-striped table-bordered table-hover table-green">
                                        <thead>
                                            <tr>
                                                <th>Profesión</th>
                                                <th>Nombre</th>
                                                <th>Área o Departamento</th>
                                                <th>Perfil Completo</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each model as |asesor|}}
                                            {{#unless asesor.isNew}}
                                            <tr class="odd gradeX">
                                                <td>{{asesor.profesion}}</td>
                                                <td>{{asesor.nombre}} {{asesor.apellidos}}</td>
                                                <td>{{asesor.departamento}}</td>
                                                <td class="center">{{#link-to "perfilasesor" asesor}}Ver más{{/link-to}}</td>
                                            </tr>
                                            {{/unless}}
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                                <!-- /.table-responsive -->
                            </div>
                            <!-- /.portlet-body -->
                        </div>
                        <!-- /.portlet -->

                    </div>
                    <!-- /.col-lg-12 -->

                </div>
                <!-- /.row -->

            </div>
            <!-- /.page-content -->

        </div>



{{outlet}}
    <script src="js/flex.js"></script>