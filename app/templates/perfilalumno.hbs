


    <div id="page-wrapper">

            <div class="page-content">

                <!-- begin PAGE TITLE ROW -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="page-title">
                            <h1>
                                Perfil 
                                <small>Alumno</small>
                            </h1>
                            <ol class="breadcrumb">
                                <li><i class="fa fa-dashboard"></i> {{#link-to 'principal'}}Inicio{{/link-to}}
                                </li>
                                <li class="active">Perfil del alumno</li>
                            </ol>
                        </div>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <!-- end PAGE TITLE ROW -->

                <div class="row">
                    <div class="col-lg-12">

                        <div class="portlet portlet-default">
                            <div class="portlet-body">
                                <ul id="userTab" class="nav nav-tabs">
                                    <li class="active"><a href="#overview" data-toggle="tab">Resumen</a>
                                    </li>
                                </ul>
                                <div id="userTabContent" class="tab-content">
                                    <div class="tab-pane fade in active" id="overview">
                                        <div class="row">
                                            <div class="col-lg-2 col-md-3">
                                                <img class="img-responsive img-profile" src="{{or model.foto "https://www.teksteshqip.com/img_upz/allart_full/63351.jpg"}}" alt="">
                                                <div class="list-group">
                                                    <a href="#" class="list-group-item active">Resumen</a>
                                                    {{#link-to "tablaalumnos" class="list-group-item"}}Regresar{{/link-to}}
                                                </div>
                                            </div>
                                            <div class="col-lg-7 col-md-5">
                                                <h1>{{model.nombre}} {{model.apellidoP}} {{model.apellidoM}}</h1>
                                                <ul class="list-inline">
                                                    <li><i class="fa fa-user fa-muted"></i> {{model.sexo}}</li>
                                                    <li><i class="">Edad</i> {{model.edad}}</li>
                                                    <li><i class="fa fa-group fa-muted"></i> {{model.nivel}}</li>
                                                </ul>
                                            </div>
                                            <div class="col-lg-6 col-md-6">
                                                <h3>Detalles del alumno</h3>
                                                 <form role="form">
                                                            <div class="form-group">
                                                                <label>Matrícula</label>
                                                                {{input type="text" class="form-control" value=model.matricula disabled=true}}
                                                            </div>
                                                            <div class="form-group">
                                                                <label>Consumo de alcohol</label>
                                                                {{input type="text" class="form-control" value=model.calcohol disabled=true}}
                                                            </div>
                                                            <div class="form-group">
                                                                <label>Consumo de tabaco</label>
                                                                {{input type="text" class="form-control" value=model.ctabaco disabled=true}}
                                                            </div>
                                                            <div class="form-group">
                                                                <label>Enfermedad crónica</label>
                                                                {{input type="text" class="form-control" value=model.enfermedad disabled=true}}
                                                            </div>
                                                            <div class="form-group">
                                                                <label>Ha tenido o actualmente tiene ayuda psicológica </label>
                                                                {{input type="text" class="form-control" value=model.apsicologica disabled=true}}
                                                            </div>
                                                            <div class="form-group">
                                                                <label>Correo Electrónico</label>
                                                                <p><i class="fa fa-envelope-o fa-muted fa-fw"></i>  <a href="#">{{model.email}}</a>
                                                                </p>
                                                            </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                 </div>
                            </div>
                            </div>
                            <!-- /.portlet-body -->
                        </div>
                        <!-- /.portlet -->


                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->

            </div>
            <!-- /.page-content -->



{{outlet}}
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script src="/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="/js/plugins/hisrc/hisrc.js"></script>
    <script src="/js/plugins/popupoverlay/jquery.popupoverlay.js"></script>
    <script src="/js/plugins/popupoverlay/defaults.js"></script>

    <script src="/js/plugins/popupoverlay/logout.js"></script>


    <script src="/js/flex.js"></script>