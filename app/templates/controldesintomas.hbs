

        <!-- begin MAIN PAGE CONTENT -->
        <div id="page-wrapper">

            <div class="page-content">

                <!-- begin PAGE TITLE ROW -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="page-title">
                            <h1>Registro de síntomas
                            </h1>
                            <ol class="breadcrumb">
                                <li><i class="fa fa-dashboard"></i> {{#link-to 'principal'}}Inicio{{/link-to}}
                                </li>
                                <li class="active">Lista de síntomas</li>
                            </ol>
                        </div>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <!-- end PAGE TITLE ROW -->

                <!-- begin ADVANCED TABLES ROW -->
                <div class="row">

                    <div class="col-lg-12">

                        <div class="portlet portlet-default">
                            <div class="portlet-heading">
                                <div class="portlet-title">
                                    <h4>Síntomas registrados</h4>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="portlet-body">
                                <div class="table-responsive">
                                    <table id="example-table" class="table table-striped table-bordered table-hover table-green">
                                        <thead>
                                            <tr>
                                                <th>Nombre</th>
                                                <th>Rating</th>
                                                <th>Ver más</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each model as |sintoma|}}
                                            <tr class="odd gradeX">
                                                <td>{{sintoma.nombre}}</td>
                                                <td>
                                                    <div class="star-rating" disabled>           
                                                        <input id="star-5" type="radio" name="rating" value="star-5">           <label for="star-5" title="5 stars">                    <i class="active fa fa-star" aria-hidden="true"></i>            
                                                                    </label>            
                                                        <input id="star-4" type="radio" name="rating" value="star-4">           <label for="star-4" title="4 stars">                    <i class="active fa fa-star" aria-hidden="true"></i>            
                                                                    </label>            
                                                        <input id="star-3" type="radio" name="rating" value="star-3">           <label for="star-3" title="3 stars">                    <i class="active fa fa-star" aria-hidden="true"></i>           
                                                                    </label>            
                                                        <input id="star-2" type="radio" name="rating" value="star-2">           <label for="star-2" title="2 stars">                    <i class="active fa fa-star" aria-hidden="true"></i>            
                                                                    </label>            
                                                        <input id="star-1" type="radio" name="rating" value="star-1">           <label for="star-1" title="1 star">                     <i class="active fa fa-star" aria-hidden="true"></i>            
                                                                    </label>        
                                                    </div>
                                                </td>
                                                <td class="center">{{#link-to "editarsintoma" sintoma.id class="btn btn-info"}}Editar{{/link-to}}</td>
                                            </tr>
                                            {{/each}}
                                            
                                        </tbody>
                                    </table>
                                </div>
                                <!-- /.table-responsive -->
                            </div>
                            <!-- /.portlet-body -->
                        </div>
                        <!-- /.portlet -->

                    </div>
                    <!-- /.col-lg-12 -->

                </div>
                <!-- /.row -->

            </div>
            <!-- /.page-content -->

        </div>


{{outlet}}
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>

<script src="/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="/js/plugins/hisrc/hisrc.js"></script>
    <script src="/js/plugins/popupoverlay/jquery.popupoverlay.js"></script>
    <script src="/js/plugins/popupoverlay/defaults.js"></script>

    <script src="/js/plugins/popupoverlay/logout.js"></script>



    <script src="/js/flex.js"></script>
    
